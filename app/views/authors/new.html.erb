<h1>New Author</h1>

    <%@author.errors.full_messages.each do |error|%>
    <ul>
        <li>
            <%=error%>
        </li>
    </ul>
    <%end%>

    <%= form_for @author do |f|%>
    <%=f.label :name%>
    <%=f.text_field :name%><br><br>
    <%=f.label :birth_year%>
    <%=f.text_field :birth_year%><br><br>
    <%=f.submit%>
    <%end%>
