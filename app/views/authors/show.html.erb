<h1><%=@author.name%> <%="(b.#{@author.birth_year})"%></h1>
    <p>Books:</p>
        <%@author.books.each do |book|%>
            <ul>
                <li>
                    <%=link_to book.name, book_path(book)%>
                </li>
            </ul>
        <%end%>
    <%=link_to "Edit #{@author.name}", edit_author_path%>
    <%= button_to "Delete #{@author.name}", @author, :method => :delete %>
